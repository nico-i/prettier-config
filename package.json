{
  "name": "@nico-i/prettier-config",
  "version": "1.0.2",
  "description": "My prettier config.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nico-i/prettier-config.git"
  },
  "private": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "prettier",
    "prettier-config",
    "prettierconfig"
  ],
  "files": [
    "index.js"
  ],
  "author": "Nico Ismaili",
  "license": "MIT",
  "peerDependencies": {
    "prettier": ">=3.3.3"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky && echo \"#!/bin/sh\nbunx --no -- commitlint --edit \\$1\" > .husky/commit-msg"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@nico-i/eslint-config": "2.7.0",
    "@types/bun": "latest",
    "commitlint-plugin-spend": "^1.0.1",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "prettier": "3.4.2",
    "typescript": "^5.6.3"
  },
  "bugs": {
    "url": "https://github.com/nico-i/prettier-config/issues"
  },
  "homepage": "https://github.com/nico-i/prettier-config#readme"
